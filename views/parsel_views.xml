<odoo>

    <record id="test_task.tree" model="ir.ui.view">
        <field name="name">test_task.tree</field>
        <field name="model">test_task</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="description"/>
                <field name="barcode_meest"/>
                <!--
                <field name="res_partner_id"/>
                <field name="res_partner_ids" widget="many2many_tags" options="{'color_field': 'color'}"/>
                -->
            </tree>
        </field>
    </record>


    <record id="test_task_main_menu" model="ir.ui.view">
        <field name="name">testtask.form ()</field>
        <field name="model">testtask</field>
        <field name="arch" type="xml">
            <form>
                <header></header>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="description"/>
                        </group>
                        <!--                        <group>-->
                        <!--                            <field name="res_partner_id"/>-->
                        <!--                            <field name="res_partner_ids" widget="many2many_tags" options="{'color_field':'color'}"/>-->
                        <!--                        </group>-->
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_test_task_window" model="ir.actions.act_window">
        <field name="name">testtask</field>
        <field name="type">ir.action.act_window</field>
        <field name="res_model">test_task</field>
        <field name="view_model">tree,form</field>
    </record>

    <menuitem id="test_task_menu_parsel"
              action="test_task.action_test_task_window"
              parent="test_task_menu_catalog"
              name="parsel" sequence="200"/>

</odoo>